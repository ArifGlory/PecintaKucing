<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="myproject.pecintakucinglampung">

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_GPS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">

        <!--
             The API key for Google Maps-based APIs is defined as a string resource.
             (See the file "res/values/google_maps_api.xml").
             Note that the API key is linked to the encryption key used to sign the APK.
             You need a different API key for each encryption key, including the release key that is used to
             sign the APK for publishing.
             You can define the keys for the debug and release targets in src/debug/ and src/release/.
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".activity.PetaRuteActivity"
            android:label="Peta Rute"></activity>
        <activity
            android:name=".activity.UbahKucingActivity"
            android:label="Ubah Data Kucing" />
        <activity android:name=".admin.UbahKesehatanActivity" />
        <activity android:name=".admin.UbahSliderActivity" />
        <activity android:name=".admin.UbahDokterActivity" />
        <activity
            android:name=".admin.ChatActivity"
            android:label="Chat" />
        <activity
            android:name=".activity.DetailKesehatanActivity"
            android:label="Detail Kesehatan" />
        <activity android:name=".admin.AddKesehatanActivity" />
        <activity
            android:name=".activity.ListKesehatanActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.MapsActivity"
            android:label="Lokasi Dokter" />
        <activity
            android:name=".admin.AddDokterActivity"
            android:label="Tambah Dokter" />
        <activity
            android:name=".admin.KelolaDokterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".admin.AddSliderActivity" />
        <activity
            android:name=".admin.KelolaSliderActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.HomeAdmin"
            android:label="Admin PKL"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.DetailPerawatActivity"
            android:label="Detail Perawat" />
        <activity
            android:name=".activity.AjukanPerawatActivity"
            android:label="Ajukan menjadi perawat" />
        <activity
            android:name=".activity.PerawatanActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.JualBeliActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.AdopsiActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.DetailcatActivity"
            android:label="Detail Kucing" />
        <activity
            android:name=".activity.AddcatActivity"
            android:label="Tambah Kucing" />
        <activity
            android:name=".activity.MycatActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.UbahProfilActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.DaftarActivity"
            android:label="Daftar Akun" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>